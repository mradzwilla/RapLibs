.rap {
	font-family: "Champion Gothic Featherweight";
	font-size: 120px;	
	color: #f2f2f2;
	text-shadow: -2px -12px red;
	display: absolute;
	width: 600px;
/*	animation: backshadow 1s infinite;
*/}

@keyframes backshadow {
	0% {
		text-shadow: -2px -12px red;
	}
	50% {
		text-shadow: -2px -16px 7px red, 3px 4px 6px #941111;
	}
	100%{
		text-shadow: -2px -9px red;
	}
}


@keyframes textgrow {

	0% {
		font-size: 54px
	}

	50% {
		font-size: 43px;
	}

	100% {
		font-size: 55px;
	}	
}


@keyframes dropIn {
   0%, 20%, 50%, 80%, 100% {transform: translateY(0);}
    40% {transform: translateY(-30px);}
    60% {transform: translateY(-15px);}
}
